            </elementSpec>
            <elementSpec ident="placeName" module="namesdates" mode="change">
               <constraintSpec ident="untrimmed-spaces-placeName" scheme="schematron">
                  <constraint>
                     <report xmlns="http://purl.oclc.org/dsdl/schematron" test="starts-with(node()[1], ' ') or ends-with(node()[last()], ' ')">Move space outside <name/>.</report>
                  </constraint>
               </constraintSpec>
               <constraintSpec ident="no-URL-in-type" scheme="schematron">
                <constraint>
                  <report xmlns="http://purl.oclc.org/dsdl/schematron" test="contains(@type, 'http')">URLs should go in @ref</report>
                </constraint>
               </constraintSpec>
               <attList>
                  <attDef ident="type" mode="replace">
                     <valList type="semi">
                        <valItem ident="باب"/>
                        <valItem ident="بركة"/>
                        <valItem ident="بيت"/>
                        <valItem ident="ترعة"/>
                        <valItem ident="ثمن"/>
                        <valItem ident="جامع"/>
