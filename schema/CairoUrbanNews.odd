                        <valItem ident="جسر"/>
                        <valItem ident="جنينة"/>
                        <valItem ident="جهة"/>
                        <valItem ident="حارة"/>
                        <valItem ident="خان"/>
                        <valItem ident="خط"/>
                        <valItem ident="درب"/>
                        <valItem ident="دكان"/>
                        <valItem ident="رواق"/>
                        <valItem ident="زقاق"/>
