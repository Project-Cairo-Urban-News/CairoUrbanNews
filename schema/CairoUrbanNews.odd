                  <constraint>
                     <report xmlns="http://purl.oclc.org/dsdl/schematron" test="starts-with(node()[1], ' ') or ends-with(node()[last()], ' ')">Move space outside <name/>.</report>
                  </constraint>
               </constraintSpec>
            </elementSpec>
            <elementSpec ident="phr" module="analysis" mode="change">
               <constraintSpec ident="untrimmed-spaces-phr" scheme="schematron">
                  <constraint>
                     <report xmlns="http://purl.oclc.org/dsdl/schematron" test="starts-with(node()[1], ' ') or ends-with(node()[last()], ' ')">Move space outside <name/>.</report>
                  </constraint>
