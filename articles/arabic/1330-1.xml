         <div resp="#eAK" xml:id="v308">
            <head><date when-custom="1330-03-01">غرة ربيع الأول ١٣٣٠</date>
               <date when="1912-02-19">١٩ فبراير ١٩١٢</date>
               <bibl><biblScope unit="issue">عدد ٢٠</biblScope>
