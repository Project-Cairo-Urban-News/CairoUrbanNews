            <row>
              <cell>١</cell>
              <cell><placeName xml:id="place-W1285019801_07">خربه</placeName> ب<placeName xml:id="place-W1285019801_08">سوق مسكه</placeName> ب<placeName xml:id="place-W1285019801_09">جهة الحنفى</placeName>
